{
  "name": "13vault-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-less-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-less-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "npm-run-all -p watch-css storybook-actual",
    "storybook-actual": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint-js": "./node_modules/.bin/eslint ./src --report-unused-disable-directives",
    "lint-js-fix": "./node_modules/.bin/eslint ./src --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@fortawesome/react-fontawesome": "0.0.20",
    "apollo-client-preset": "^1.0.5",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.2",
    "dotenv-cli": "^1.4.0",
    "graphql": "^0.12.3",
    "graphql-request": "^1.6.0",
    "graphql-tag": "^2.9.2",
    "less": "^2.7.3",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-less-chokidar": "^0.1.2",
    "npm-run-all": "^4.1.3",
    "numbro": "^1.11.1",
    "react": "16.2.0",
    "react-apollo": "2.0.4",
    "react-dom": "16.2.0",
    "react-fontawesome": "^1.6.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "recompose": "^0.26.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-links": "^3.4.5",
    "@storybook/react": "^3.4.5",
    "casual": "^1.5.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "jsx-loader": "^0.13.2",
    "less-loader": "^4.1.0",
    "storybook-addon-apollo-graphql": "^0.1.2",
    "storybook-router": "^0.3.4",
    "style-loader": "^0.19.1"
  }
}
